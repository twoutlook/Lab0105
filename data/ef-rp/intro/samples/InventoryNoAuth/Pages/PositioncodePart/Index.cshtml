    <tbody>
@foreach (var item in Model.MCpositioncodePart) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cpositioncode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cposition)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cinvcode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Part)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rank)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RankFinal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Iqty)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
