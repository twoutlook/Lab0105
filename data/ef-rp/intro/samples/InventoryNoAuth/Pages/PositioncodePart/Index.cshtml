@page
@model InventoryNoAuth.Pages.PositioncodePart.IndexModel

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MCpositioncodePart[0].Cpositioncode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MCpositioncodePart[0].Cposition)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MCpositioncodePart[0].Cinvcode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MCpositioncodePart[0].Part)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MCpositioncodePart[0].Rank)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MCpositioncodePart[0].RankFinal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MCpositioncodePart[0].Iqty)
            </th>
            <th></th>
        </tr>
